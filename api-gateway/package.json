    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-2": "^6.24.1",
    "nodemon": "^2.0.1"
  },
  "dependencies": {
    "apollo-server-express": "^2.14.2",
    "axios": "^0.19.0",
    "cross-env": "^6.0.3",
    "express": "^4.17.1",
    "graphql": "^14.5.8",
    "graphql-tools": "^4.0.6",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "merge-graphql-schemas": "^1.7.3",
    "moment": "^2.24.0",
    "morgan": "^1.9.1"
